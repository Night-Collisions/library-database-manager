<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="libapp.view.UserController">
   <children>
      <TableView fx:id="table" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columns>
          <TableColumn fx:id="id" minWidth="30" maxWidth="100" text="ID" />
          <TableColumn fx:id="surname" minWidth="50" text="Фамилия" />
            <TableColumn fx:id="name" minWidth="50" text="Имя" />
            <TableColumn fx:id="patronymic" minWidth="50" text="Отчество" />
            <TableColumn fx:id="sex" minWidth="50" text="Пол" />
            <TableColumn fx:id="login" minWidth="50" text="Логин" />
            <TableColumn fx:id="birthday" minWidth="50" text="День рождения" />
            <TableColumn fx:id="type" minWidth="50" text="Группа" />
            <TableColumn fx:id="phonenumber" minWidth="50" text="Телефон" />
            <TableColumn fx:id="email" minWidth="50" text="Email" />
        </columns>
          <columnResizePolicy>
              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
          </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
