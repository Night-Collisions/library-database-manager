<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="libapp.view.NotificationController">
   <children>
      <TableView fx:id="table" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columns>
          <TableColumn fx:id="id" minWidth="30" maxWidth="100" text="ID" />
          <TableColumn fx:id="userId" minWidth="50" maxWidth="100"  text="User ID" />
            <TableColumn fx:id="login" minWidth="50" text="Логин" />
            <TableColumn fx:id="potentialType" minWidth="50" text="Перевести в" />
            <TableColumn fx:id="phonenumber" minWidth="50" text="Телефон" />
            <TableColumn fx:id="email" minWidth="50" text="Email" />
        </columns>
          <columnResizePolicy>
              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
          </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
