<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="70.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" spacing="5" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <ComboBox fx:id="combobox" prefWidth="9999999999999" AnchorPane.leftAnchor="0" />
         <HBox alignment="BOTTOM_RIGHT" spacing="10">
            <Button fx:id="accept" minWidth="100" onAction="#applyChange" text="Применить" />
            <Button fx:id="reject" minWidth="100"  onAction="#closeWindow" text="Отмена" />
         </HBox>
      </VBox>
   </children>
</AnchorPane>
