<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="libapp.view.EditorController">
   <children>
      <TableView fx:id="table" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columns>
          <TableColumn fx:id="id" minWidth="30" maxWidth="100"  text="ID" />
          <TableColumn fx:id="surname" minWidth="40" text="Фамилия" />
            <TableColumn fx:id="name" minWidth="40" text="Имя" />
            <TableColumn fx:id="patronymic" minWidth="40" text="Отчество" />
            <TableColumn fx:id="birthday" minWidth="40" text="Дата рождения" />
            <TableColumn fx:id="deathday" minWidth="40" text="Дата смерти" />
            <TableColumn fx:id="phonenumber" minWidth="40" text="Номер телефона" />
            <TableColumn fx:id="email" minWidth="40" text="E-mail" />
        </columns>
          <columnResizePolicy>
              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
          </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
