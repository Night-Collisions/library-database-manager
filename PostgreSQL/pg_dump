--
-- Name: check_users_authors(); Type: FUNCTION; Schema: public; Owner: username
--

CREATE FUNCTION public.check_users_authors() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
BEGIN
  UPDATE users u
  SET type = 3
  WHERE u.users_id = NEW.users_id
    AND u.type = 4;

  IF NOT FOUND THEN
    RAISE EXCEPTION 'Пользователь не может быть привязан.';
  END IF;

  RETURN NEW;
END;
$$;


ALTER FUNCTION public.check_users_authors() OWNER TO username;

