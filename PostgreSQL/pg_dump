    birth_date date,
    death_date date,
    phone_number bigint,
    email character varying(100),
    CONSTRAINT authors_check CHECK ((birth_date <= death_date)),
    CONSTRAINT authors_sex_check CHECK ((sex = ANY (ARRAY[0, 1, 2])))
);


